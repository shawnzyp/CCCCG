{
  "rules": {
    ".read": false,
    ".write": false,
    "characters": {
      "$uid": {
        ".read": "auth != null && (auth.uid === $uid || auth.token.dm === true)",
        ".write": "auth != null && (auth.uid === $uid || auth.token.dm === true)"
      }
    },
    "autosaves": {
      "$uid": {
        ".read": "auth != null && (auth.uid === $uid || auth.token.dm === true)",
        ".write": "auth != null && (auth.uid === $uid || auth.token.dm === true)"
      }
    }
  }
}
